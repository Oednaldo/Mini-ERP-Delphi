unit ERP_Delphi_Routers;

interface
Type
TRouters = class
  private
  public
    Constructor Create;
    destructor Destroy; override;
end;
var
  Router : TRouters;
implementation

{ TRouters }

constructor TRouters.Create;
begin

end;

destructor TRouters.Destroy;
begin

  inherited;
end;
initialization
  Router := TRouters.Create	;

finalization
Router.Free;

end.
